{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "-ms-touch-action",
          "scrollbar-width"
        ]
      }
    ]
  }
}